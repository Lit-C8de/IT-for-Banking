{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-fraudes-ibk"
		},
		"LS_ADLS_DataLakeHouse_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"LS_ADLS_DataLakeHouse\""
		},
		"LS_ADLS_DataLakeHouse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakefraudes.dfs.core.windows.net/"
		},
		"LS_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-fraudes-dev.vault.azure.net/"
		},
		"LS_SQLOnPremise_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-2L05QEO"
		},
		"LS_SQLOnPremise_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "bank_transactions"
		},
		"LS_SQLOnPremise_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ADFUser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-fraudes-ibk_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_DataLakeHouse_accountKey": {
						"value": "[parameters('LS_ADLS_DataLakeHouse_accountKey')]"
					},
					"LS_ADLS_DataLakeHouse_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_DataLakeHouse_properties_typeProperties_url')]"
					},
					"LS_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_SQLOnPremise_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLOnPremise_properties_typeProperties_server')]"
					},
					"LS_SQLOnPremise_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLOnPremise_properties_typeProperties_database')]"
					},
					"LS_SQLOnPremise_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLOnPremise_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}